#include <stdio.h> //배열을 역순으로 출력

int binary(int n)
{
	int a,b,c,i,q,count; count = 0;
	int arr[100];
	while(n!=0)
	{
	arr[count] = n%2;
	count = count+1;
	n = n/2; 
	}

	printf("\n");
	
	return 0;	
}

int main()
{
	int a,b,i,total;
	total = 1;
	scanf("%d",&a);
	for(b = 0; b<a; b++)
	{
		total = total*2;
	}
	
	binary(14);
	binary(17);
	

//	for(i = 0; i<total; i++)
//	{
//		binary(i);
//	}

	return 0;
}
